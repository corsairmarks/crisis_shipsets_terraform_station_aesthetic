	name = "extra_dimensional_01_aesthetic_terraform_station_entity"
	pdxmesh = "extra_dimensional_01_station_small_mesh"
	locator = { name = "part1" position = { 0 0 0 } }
	locator = { name = "beam1" position = { 0 3 0 } scale = 110 rotation = { 0 -90 0 } }
	default_state = "idle"
