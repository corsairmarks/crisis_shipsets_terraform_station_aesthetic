		size 					= @terraforming_station
		uv_animation_speed 		= 0.2
		uv_animation_direction 	= { 0.0 0.02 }
		bloom_factor = @exd_bloom_factor
	}
	attach = { "beam1" = "terraforming_effect_entity" }
}

